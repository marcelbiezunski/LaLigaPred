   ],
   "source": [
    "import pandas as pd\n",
    "matches = pd.read_csv('matches.csv', index_col=0)\n",
    "matches.dtypes"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Data optimization; creating predictors"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "matches['date'] = pd.to_datetime(matches['date'])\n",
    "matches['venue_code'] = matches['venue'].astype('category').cat.codes #categorise to home=1 and away=0\n",
    "matches['opponent_code'] = matches['opponent'].astype('category').cat.codes #each opponent gets its code\n",
    "matches['hour'] = matches['time'].str.split(':').str[0].astype(int) #converting hours to int values\n",
    "matches['day_code'] = matches['date'].dt.dayofweek #codes days of the week to int value\n",
    "\n",
    "matches['referee_code'] = matches['referee'].astype('category').cat.codes\n",
    "matches['captain_code'] = matches['captain'].astype('category').cat.codes\n",
    "\n",
    "matches['xg_diff'] = matches['xg'] - matches['xga']\n",
    "\n",
    "matches['shot_accuracy'] = matches['sot'] / matches['sh']\n",
    "matches['shot_accuracy'] = matches['shot_accuracy'].fillna(0)  # if sh==0\n",
    "\n",
    "matches['round_code'] = matches['round'].astype('category').cat.codes\n",
    "\n",
    "matches['form_code'] = matches['formation'].astype('category').cat.codes\n",
    "matches['oppform_code'] = matches['opp formation'].astype('category').cat.codes\n",
    "\n",
    "\n",
    "\n",
    "matches['target'] = (matches['result'] == 'W').astype('int') #so the win converts into int=1, rest equals 0"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "ML model implementation (Random Forest Classifier)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.ensemble import RandomForestClassifier\n",
    "\n",
    "rf = RandomForestClassifier(n_estimators=50, min_samples_split=10, random_state=1)\n",
    "\n",
    "training_set = matches[matches['date'] < '2025-01-01']\n",
    "test_set = matches[matches['date'] > '2025-01-01']\n",
    "\n",
    "predictors = ['venue_code', 'opponent_code', 'hour', 'day_code', 'referee_code', 'captain_code',\n",
    "              'xg_diff', 'shot_accuracy', 'round_code', 'form_code', 'oppform_code', 'xg', 'xga']\n",
    "\n",
    "rf.fit(training_set[predictors], training_set['target'])\n",
    "\n",
    "predictions = rf.predict(test_set[predictors])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Accuracy: 0.763\n",
      "Precision: 0.681\n"
     ]
    }
   ],
   "source": [
    "from sklearn.metrics import accuracy_score\n",
    "from sklearn.metrics import precision_score\n",
    "\n",
    "accuracy = accuracy_score(test_set['target'], predictions)\n",
    "precision = precision_score(test_set['target'], predictions)\n",
    "\n",
    "print(f\"Accuracy: {round(accuracy, 3)}\\nPrecision: {round(precision, 3)}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "          Predicted 0  Predicted 1\n",
      "Actual 0           87           15\n",
      "Actual 1           22           32\n"
     ]
    }
   ],
   "source": [
    "from sklearn.metrics import confusion_matrix\n",
    "\n",
    "actual = test_set['target']\n",
    "predicted = predictions\n",
    "\n",
    "cm = confusion_matrix(actual, predicted)\n",
    "\n",
    "cm_df = pd.DataFrame(cm, index=[f'Actual {i}' for i in range(len(cm))],\n",
    "                     columns=[f'Predicted {i}' for i in range(len(cm[0]))])\n",
    "\n",
    "print(cm_df)\n"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "New predictors = rolling average"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "def rolling_averages(group, cols, new_cols):\n",
    "    group = group.sort_values('date')\n",
    "    rolling_states = group[cols].rolling(3, closed='left').mean()\n",
    "    group[new_cols] = rolling_states\n",
    "    group = group.dropna(subset=new_cols)\n",
    "    return group"
   ]
  },
